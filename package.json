{
  "name": "update-nodes",
  "description": "Update Recommended Node.js Versions",
  "version": "0.0.13",
  "main": "dist/cli",
  "license": "MIT",
  "repository": "fisker/update-nodes",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "update-nodes": "dist/cli"
  },
  "dependencies": {
    "enquirer": "2.3.2",
    "execa": "3.4.0",
    "listr": "0.14.3",
    "meow": "5.0.0",
    "nodejs-recommended-versions": "1.0.9",
    "ora": "4.0.3",
    "signale": "1.4.0",
    "update-notifier": "3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@fisker/commitlint-config": "1.1.4",
    "@fisker/eslint-config": "1.5.0",
    "@fisker/husky-config": "1.1.5",
    "@fisker/lint-staged-config": "1.0.8",
    "@fisker/prettier-config": "1.0.24",
    "cz-conventional-changelog": "3.0.2",
    "del-cli": "3.0.0",
    "eslint": "6.7.2",
    "esm": "3.2.25",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "markdownlint-cli": "0.19.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "rollup": "1.27.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "clean": "run-p clean:*",
    "clean:dist": "del-cli dist",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s lint build format dist"
  },
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false
}
