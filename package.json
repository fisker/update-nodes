{
  "name": "update-nodes",
  "version": "2.0.5",
  "description": "Update recommended Node.js versions",
  "homepage": "https://github.com/fisker/update-nodes#readme",
  "bugs": {
    "url": "https://github.com/fisker/update-nodes/issues"
  },
  "repository": "fisker/update-nodes",
  "funding": "https://github.com/fisker/update-nodes?sponsor=1",
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false,
  "type": "module",
  "bin": "./cli.js",
  "files": [
    "cli.js",
    "lib"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:js": "echo no need",
    "clean": "run-p clean:*",
    "clean:dist": "del-cli dist",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,cjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:package-json": "yarn run format:package-json --check",
    "lint:prettier": "prettier \"**/*.{css,html,js,cjs,mjs,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s lint build format test dist",
    "test": "ava",
    "test-coverage": "nyc yarn test"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "verbose": true
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "dependencies": {
    "enquirer": "2.3.6",
    "esm-utils": "1.1.0",
    "execa": "5.1.1",
    "listr": "0.14.3",
    "meow": "10.0.1",
    "nodejs-recommended-versions": "4.0.1",
    "ora": "5.4.1",
    "signale": "1.4.0",
    "update-notifier": "5.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@fisker/commitlint-config": "1.3.5",
    "@fisker/eslint-config": "7.0.1",
    "@fisker/husky-config": "4.0.5",
    "@fisker/lint-staged-config": "3.0.5",
    "@fisker/prettier-config": "4.0.6",
    "ava": "3.15.0",
    "cz-conventional-changelog": "3.3.0",
    "del-cli": "4.0.1",
    "eslint": "7.31.0",
    "husky": "7.0.1",
    "lint-staged": "11.1.1",
    "markdownlint-cli": "0.28.1",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.3.2",
    "sort-package-json": "1.50.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
