{
  "name": "update-nodes",
  "description": "Update Recommend Node.js Versions",
  "version": "0.0.2",
  "main": "index.js",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "bin"
  ],
  "bin": {
    "update-nodes": "bin/cli"
  },
  "dependencies": {
    "enquirer": "2.3.1",
    "execa": "2.0.4",
    "listr": "0.14.3",
    "meow": "5.0.0",
    "nodejs-recommended-versions": "1.0.2",
    "ora": "3.4.0",
    "signale": "1.4.0",
    "update-notifier": "3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "8.1.0",
    "@fisker/commitlint-config": "1.1.2",
    "@fisker/eslint-config": "1.2.16",
    "@fisker/husky-config": "1.1.1",
    "@fisker/lint-staged-config": "1.0.5",
    "@fisker/prettier-config": "1.0.18",
    "cz-conventional-changelog": "3.0.2",
    "del-cli": "2.0.0",
    "eslint": "6.3.0",
    "esm": "3.2.25",
    "husky": "3.0.5",
    "lint-staged": "9.2.5",
    "markdownlint-cli": "0.18.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "rollup": "1.20.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "clean": "del bin",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s lint build format dist"
  },
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false
}
